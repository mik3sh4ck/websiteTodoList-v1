<%- include('header') %>
        <div class="box" id="heading">
            <h1><%= listTitle %></h1>
        </div>

        <div class="box">
            
                <% for(i = 0; i < newListItem.length ; i++){%>
                    <div class="item">
                        <input type="checkbox">
                        <p><%= newListItem[i] %></p>
                    </div>
                <% }%>

                <form action="/" method="post" class="item">
                    <input type="text" name="newToDoList" placeholder="New Item" autocomplete="off">
                    <button type="submit" name="button" value=<%= listTitle %>>+</button>
                </form>
        </div>
<%- include('footer') %>